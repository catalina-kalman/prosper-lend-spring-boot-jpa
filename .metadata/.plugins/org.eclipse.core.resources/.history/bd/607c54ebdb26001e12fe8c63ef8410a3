package com.model.persistence;

import javax.transaction.Transactional;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Modifying;
import org.springframework.data.jpa.repository.Query;

import com.dto.entity.UserLogin;

public interface UserLoginDao extends JpaRepository<UserLogin, Integer>{

	@Transactional
	@Modifying
	@Query(nativeQuery = true,value = "insert into userlogin values(?,?,?,?,?)")
	public int insertUserLoginData(int userLoginId,String userName,String passcode,String salt,String role);
}
